<div class="container">
    <table class="table table-hover table-striped">
  <thead class="thead-dark">
    <tr>
      <th scope="col">S.No.</th>
      <th scope="col">Roll-No</th>
      <th scope="col">Name</th>
      <th scope="col">Phone Number</th>
      <th scope="col">Component Details</th>
      <th scope="col">Given</th>
    </tr>
  </thead>
  <tbody>
<% var i = 1 %>
<%        returned.forEach(ret=>{ %>
    <tr>
      <th scope="row"><%= i %> </th>
      <% i += 1 %> 
      <td><%= ret[0] %> </td>
      <td><%= ret[1] %> </td>
      <td><%= ret[2] %> </td>
      <td>
        <%= ret[3] %> 
      </td>
      <td>
        <form action="/admin/returned" method="POST" >
          <button class="btn btn-success" name="transId" value="<%= ret[4] %>">Submit</button>
        </form>
      </td>
    </tr>
<%        }) %>

  </tbody>
</table>
</div>

